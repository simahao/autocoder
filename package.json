{
  "name": "autocoder",
  "displayName": "AutoCoder",
  "description": "builder getter setter toString",
  "version": "0.0.1",
  "publisher": "simahao",
  "license": "MIT",
  "icon": "image/icon.png",
  "keywords": [
    "java",
    "setter",
    "getter",
    "toString",
    "builder"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simahao/autocoder"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.AutoCoderAll",
    "onCommand:extension.AutoCoderSetAndGet",
    "onCommand:extension.AutoCoderBuilder"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.AutoCoderAll",
        "title": "AutoCoder: Generate All"
      },
      {
        "command": "extension.AutoCoderSetAndGet",
        "title": "AutoCoder: Generate Setter And Getter"
      },
      {
        "command": "extension.AutoCoderBuilder",
        "title": "AutoCoder: Generate Builder"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.AutoCoderAll",
          "when": "editorLangId == java",
          "group": "autocoder@1"
        },
        {
          "command": "extension.AutoCoderSetAndGet",
          "when": "editorLangId == java",
          "group": "autocoder@2"
        },
        {
          "command": "extension.AutoCoderBuilder",
          "when": "editorLangId == java",
          "group": "autocoder@3"
        }
      ]
    },
    "configuration": {
      "title": "AutoCoder Configuration",
      "properties": {
        "autocoder.indent": {
          "type": "string",
          "default": "tab",
          "enum": [
            "tab",
            "space"
          ],
          "description": "insert tab or space for format."
        },
        "autocoder.space": {
          "type": "integer",
          "default": 4,
          "enum": [
            2,
            4,
            8
          ],
          "description": "if indent is space, you can set space for format,eg 4."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "java-ast": "^0.1.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.8.0",
    "typescript": "^3.2.2",
    "vscode": "^1.1.26"
  },
  "dependencies": {
    "java-ast": "^0.1.0"
  }
}
